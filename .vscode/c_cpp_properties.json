{
    "configurations": [
      {
        "name": "esp32",
        "cStandard": "c11",
        "cppStandard": "c++20",
        "compileCommands": "${workspaceFolder}/${config:esp32.activeProject}/build/compile_commands.json",
        "includePath": [
          "/opt/esp/idf/components/**",
          "${workspaceFolder}/**",
          "${workspaceFolder}/examples/**",
          "${workspaceFolder}/**/managed_components/**"
        ],
        "browse": {
          "path": [
            "/opt/esp/idf/components/**",
            "${workspaceFolder}/**",
            "${workspaceFolder}/examples/**",
            "${workspaceFolder}/**/managed_components/**"
          ],
          "limitSymbolsToIncludedHeaders": false
        }
      }
    ],
    "version": 4
}